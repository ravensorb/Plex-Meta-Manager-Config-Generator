metadata:               #Posters found here: https://theposterdb.com/user/musikmann2000
{% for item in items %}
    "{{ item.metadata.title}} ({{ item.metadata.year }})":
        year: {{ item.metadata.year }}
        title: "{{ item.metadata.title }}"
        url_poster: https://theposterdb.com/api/assets/
        # IDs: {{ item.metadata.guids | map(attribute="id") | list | join(', ')}}
        # tpdb: {{ item.metadata | generateTpDbSearchUrl }}
        # tmdb: https://www.themoviedb.org/tv/{{ item.metadata | getItemGuidByName("tmdb") }}
        # tvdb: https://thetvdb.com/search?menu%5Btype%5D=series&query={{ item.metadata | getItemGuidByName("tvdb") }}
        # imdb: https://www.imdb.com/title/{{ item.metadata | getItemGuidByName("imdb") }}
        seasons:
        {%- for season in item.seasons %}
            {{ season.seasonNumber }}:
                url_poster: https://theposterdb.com/api/assets/
                # IDs: {{ season.guids | map(attribute="id") | list | join(', ')}}
                # {{ season | generateTpDbSearchUrl }}
        {%- endfor %}
        
{% endfor %}
