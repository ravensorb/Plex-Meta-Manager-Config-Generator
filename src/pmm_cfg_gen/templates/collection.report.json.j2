{
    "items": [{% for item in items %}
        {
            "title" : {{ item.title }},
            "urls": {
                "thePosterDatabase": "{{ item.searchUrl }}",
                "theMovieDatabase": "https://www.themoviedb.org/search/collection?query={{ item.title | urlencode}}",
                "theTvDatabase": "https://thetvdb.com/search?menu%5Btype%5D=list&query={{ item.title | urlencode }}",
                "imDb": "https://www.imdb.com/find/?s=tt&ttype={% if item.metadata.type == "movie" %}ft{% elif item.metadata.type == "show" %}tv{% else %}{% endif %}={{ item.title | urlencode }}"
            }
        }{% if not loop.last %},{% endif %}
    {% endfor %}],
    "stats" : {{ stats | formatJson }},
    "processingTime" : {{ processingTime | formatJson }}
}
